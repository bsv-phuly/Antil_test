{
  "name": "vue-sonner",
  "version": "1.0.4",
  "type": "module",
  "author": "xiaoluoboding <xiaoluoboding@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build:docs": "vite build --mode docs",
    "build:lib": "vite build --mode lib && vue-tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaoluoboding/vue-sonner.git"
  },
  "homepage": "https://github.com/xiaoluoboding/vue-sonner",
  "files": [
    "lib",
    "volar.d.ts"
  ],
  "main": "./lib/vue-sonner.cjs",
  "module": "./lib/vue-sonner.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/vue-sonner.js",
      "require": "./lib/vue-sonner.cjs"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@iconify/json": "^2.2.172",
    "@types/node": "^18.19.8",
    "@unocss/reset": "^0.58.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^10.7.2",
    "@vueuse/head": "^2.0.0",
    "clean-css": "^5.3.3",
    "highlight.js": "^11.9.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-icons": "^0.18.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.12",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
}